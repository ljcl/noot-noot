{
  "name": "noot.space",
  "version": "2.0.0",
  "description": "Maximum noot",
  "author": "Luke Clark <luke@lukeclark.com.au> (https://lukeclark.com.au)",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "node-sass": "^3.8.0",
    "webpack": "^1.13.1"
  },
  "main": "app.js",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "start": "node app.js",
    "build:css": "node-sass src/noot.scss -o public",
    "build:js": "webpack -p",
    "prepublish": "npm run build:css && npm run build:js"
  },
  "paths": {
    "styles": {
      "files": "style.scss",
      "dest": "app"
    },
    "scripts": {
      "files": "script.js",
      "dest": "app"
    }
  },
  "dependencies": {
    "body-parser": "^1.13.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "newrelic": "^1.28.3",
    "pug": "^2.0.0-beta4"
  }
}
